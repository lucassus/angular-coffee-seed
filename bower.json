{
  "name": "Angular grunt seed",
  "version": "0.0.1",
  "dependencies": {
    "jquery": "2.0.3",
    "lodash": "2.4.1",

    "angular": "1.2.4",
    "angular-resource": "1.2.4",
    "angular-animate": "1.2.4",
    "angular-ui-router": "0.2.7 ",

    "bootstrap": "3.0.3",
    "font-awesome": "4.0.3"
  },
  "devDependencies": {
    "angular-mocks": "1.2.4",
    "sinon": "1.7.3"
  },
  "exportsOverride": {
    "angular-ui-router": {
      "/": ["release/angular-ui-router.js"]
    },
    "font-awesome": {
      "css": ["css/font-awesome.css", "css/font-awesome-ie7.css"],
      "fonts": ["fonts/*"]
    },
    "lodash": {
      "/": ["dist/lodash.js"]
    },
    "sinon": {
      "/": ["lib/sinon.js"],
      "lib": ["lib/**/*.js"]
    }
  }
}
